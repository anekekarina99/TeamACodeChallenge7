<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teamacodechallenge7">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_apps"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_apps_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamACodeChallenge7">
        <activity android:name=".ui.loginPage.LoginAct"></activity>
        <activity android:name=".ui.splashScreen.SplashScreenActivity"></activity>
        <activity android:name=".ui.profileteman.ProfileTeman">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.landingPage.LandingPageActivity" />
    </application>

</manifest>